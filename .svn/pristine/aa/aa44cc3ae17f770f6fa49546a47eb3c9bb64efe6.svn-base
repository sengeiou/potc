<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >


    <RelativeLayout
        android:id="@+id/head"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:background="@color/white">

        <TextView
            android:id="@+id/teb_head_title"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@color/textclolr_title_blcak"
            android:textSize="16dp"
            android:text="@string/setting_title_equipment_head"
            />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#f5f5f6"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn_print"
        android:layout_width="match_parent"
        android:layout_below="@+id/head"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="35dp"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/txt_print"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical|left"
            android:text="@string/device_print"
            android:textColor="@color/textclolr_title_gray"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/value_print"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/txt_print"
            android:gravity="center_vertical|left"
            android:text="@string/setting_title_equipment_unset"
            android:textColor="@color/textclolr_title_hit"
            android:textSize="14dp" />

        <ImageView
            android:layout_width="10dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/right"
            />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#f5f5f6"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn_potc"
        android:layout_width="match_parent"
        android:layout_below="@+id/btn_print"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="35dp"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/txt_potc"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical|left"
            android:text="@string/device_potc"
            android:textColor="@color/textclolr_title_gray"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/value_potc"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/txt_potc"
            android:gravity="center_vertical|left"
            android:text="@string/setting_title_equipment_unset"
            android:textColor="@color/textclolr_title_hit"
            android:textSize="14dp" />

        <ImageView
            android:layout_width="10dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/right"
            />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#f5f5f6"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn_fada"
        android:layout_width="match_parent"
        android:layout_below="@+id/btn_potc"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="35dp"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/txt_fada"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical|left"
            android:text="@string/device_fada"
            android:textColor="@color/textclolr_title_gray"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/value_fada"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/txt_fada"
            android:gravity="center_vertical|left"
            android:text="@string/setting_title_equipment_unset"
            android:textColor="@color/textclolr_title_hit"
            android:textSize="14dp" />

        <ImageView
            android:layout_width="10dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/right"
            />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#f5f5f6"/>
    </RelativeLayout>

</RelativeLayout>
