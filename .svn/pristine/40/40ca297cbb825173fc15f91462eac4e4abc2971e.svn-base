<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000">

    <RelativeLayout
        android:id= "@+id/layer2"
        android:layout_width="360dp"
        android:layout_height="500dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/bg_report_creat">

        <RelativeLayout
            android:id="@+id/head"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:background="@drawable/bg_report_creat_head">

            <RelativeLayout
                android:id="@+id/layertitle"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="18dp"
                android:background="#00000000">

                <ImageView
                    android:id="@+id/imgcreat"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/report" />.

                <TextView
                    android:id="@+id/title"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/imgcreat"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/test_center_temp"
                    android:textColor="@color/white"
                    android:textSize="16dp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layerclose"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="18dp"
                android:background="#00000000">

                <ImageView
                    android:id="@+id/imgclose"
                    android:layout_width="14dp"
                    android:layout_height="14dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/colse" />.

            </RelativeLayout>

        </RelativeLayout>

        <ListView
            android:id="@+id/templist"
            android:layout_width="320dp"
            android:layout_height="411dp"
            android:layout_below="@+id/head"
            android:layout_centerHorizontal="true"
            android:background="#00000000"
            android:dividerHeight="1dp"
            android:divider="@color/textclolr_title_gray"
            />

        <RelativeLayout
            android:id="@+id/line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/templist"
            android:background="#d7d7d7" />

        <RelativeLayout
            android:id="@+id/end"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/line"
            android:layout_marginTop="9dp"
            android:background="#00000000">

            <TextView
                android:id="@+id/btnok"
                android:layout_width="80dp"
                android:layout_height="32dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="30dp"
                android:gravity="center_vertical|center_horizontal"
                android:text="@string/button_word_ok"
                android:background="@drawable/btn_green"
                android:textColor="@color/white"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/btncancle"
                android:layout_width="80dp"
                android:layout_height="32dp"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:layout_toLeftOf="@+id/btnok"
                android:gravity="center_vertical|center_horizontal"
                android:background="@drawable/btn_empty"
                android:text="@string/button_word_cancle"
                android:textColor="@color/textclolr_title_gray"
                android:textSize="16dp" />

        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>